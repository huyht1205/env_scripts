#!/bin/bash
export PS1='\[\033[31;1m\]\w\[\033[0m\]\$ '

alias cdenv='cd /data'
alias cdwork='cd /data/50-workspace'
alias cdgit='cd /data/10-git_repo'
alias Rebuild='bitbake -c cleansstate beginner-tutorials && bitbake beginner-tutorials'
alias ConnectSerial='picocom -b 115200 /dev/ttyUSB0'

function DoSB2Conf {
	sb2-init -n -c /usr/bin/qemu-arm $1 /usr/bin/arm-linux-gnueabihf-gcc
}

source /usr/share/bash-completion/completions/git
