#!/bin/bash
export PS1='\[\033[35;1m\]\w\[\033[0m\]\$ '
export MAC_DOWNLOADS='hoangtrunghuy@192.168.15.5:/Users/hoangtrunghuy/Downloads'

alias picocom='picocom -e q'
alias ConnectSerial='picocom -b 115200 /dev/ttyUSB0'
alias MountMac='sshfs hoangtrunghuy@lh-mac.local:/data/workspace /data/workspace/'

source /usr/share/bash-completion/bash_completion

function DoShare
{
	mount.vboxsf vboxsf /data/workspace/
}
